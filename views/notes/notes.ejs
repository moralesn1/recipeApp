<!DOCTYPE html>
<html lang="en">
  <head>
    <title></title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="css/style.css" rel="stylesheet">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" integrity="sha384-JcKb8q3iqJ61gNV9KGb8thSsNjpSL0n8PARn9HuZOnIxN0hoP+VmmDGMN5t9UJ0Z" crossorigin="anonymous">

  </head>
  <body>
    <%- include('../partials/_headingnavbar')%>
    <div class="container">
      <h1 class="mt-3 mb-5">Notes</h1>
      <% notes.forEach(note => { %>
      <form action="/notes/<%= note.id%>?_method=DELETE" method="POST">
        <li class="list-group-item">
          <input type="checkbox" class="mr-3" onChange="this.form.submit()">
          <%= note.text %>
        </li>
      </form>

 
        
        
      <% }) %>
      <form action="/notes" method="POST" class="mt-5">
        <%- include('_note_form')%>
      </form>
    </div>




  </body>
</html>